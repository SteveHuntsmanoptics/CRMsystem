# unzip, then:
pnpm install

# set DB for both apps
cp apps/starter/.env.example apps/starter/.env
cp apps/web/.env.example apps/web/.env
# edit both .env files: DATABASE_URL=postgres://...

# apply schema + seed
cd apps/starter
pnpm prisma migrate deploy
pnpm prisma db push
pnpm ts-node prisma/seed.ts
cd ../..

# run the UI
cd apps/web
pnpm prisma generate
pnpm dev
